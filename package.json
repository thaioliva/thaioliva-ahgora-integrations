{
  "name": "ahgora-integrator",
  "version": "1.0.0",
  "description": "#### Stack - React Router - Axios - Redux",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --watch",
    "dev": "webpack --config webpack.config.js --watch",
    "start": "webpack && webpack-dev-server --config webpack.config.js --mode development --open --hot --watch",
    "build:development": "webpack --config webpack.config.development.js --mode production",
    "build:local": "webpack --config webpack.config.test.js --mode production",
    "build:production": "webpack --config webpack.config.production.js --mode production"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx,mjs}"
    ],
    "setupTestFrameworkScriptFile": "./setupTests.js",
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.(css|scss)$": "./node_modules/jest-css-modules",
      "^.+\\.(js|jsx|mjs)$": "./node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "author": "@workandcode",
  "license": "ISC",
  "homepage": "http:///",
  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cache-loader": "^1.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "cypress": "^3.0.1",
